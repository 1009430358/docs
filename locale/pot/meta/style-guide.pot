# 4d9364ddadef496f9380f31508353bf5
msgid "Distributed System Terms"
msgstr ""

#: ../source/meta/style-guide.txt:469
# 24a4d92fbf2846e694d63bc3bdf311ca
msgid "Refer to partitioned systems as \"sharded clusters.\" Do not use shard clusters or sharded systems."
msgstr ""

#: ../source/meta/style-guide.txt:472
# 6eeccd4d8939491d8ff3a60c94b839e2
msgid "Refer to configurations that run with replication as \"replica sets\" (or \"master/slave deployments\") rather than \"clusters\" or other variants."
