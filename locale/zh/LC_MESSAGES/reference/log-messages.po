msgid "``F``"
msgstr ""

#: ../source/reference/log-messages.txt:54
msgid "Fatal"
msgstr ""

#: ../source/reference/log-messages.txt:56
msgid "``E``"
msgstr ""

#: ../source/reference/log-messages.txt:57
msgid "Error"
